<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="cloud" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2013-10.2.2">
  <WorkerRole name="VideoStore.ContentManagement" vmsize="Small">
    <Imports>
      <Import moduleName="Diagnostics" />
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
    </Imports>
    <LocalResources>
      <LocalStorage name="endpoints" cleanOnRoleRecycle="true" sizeInMB="10" />
    </LocalResources>
  </WorkerRole>
</ServiceDefinition>